#include <bits/stdc++.h> 
using namespace std; 
  
int main() 
{ 
    int i,b;
    int square = 0; 
    float mean = 0.0, root = 0.0; 
    printf("enter the no of digits");
    scanf("%d",&b);
    float arr[20];
    printf("enter the values");
    for(i=0;i<b;i++)
    {
        scanf("%f",&arr[i]);
    }
    for (int i = 0; i < b; i++) { 
        square += pow(arr[i], 2);} 
    
    mean = (square / (float)(b)); 
  
    root = sqrt(mean); 
  
    cout<<root;  
}
